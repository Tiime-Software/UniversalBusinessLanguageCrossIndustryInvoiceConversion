{
    "name": "tiime/universal-business-language-cross-industry-invoice-conversion",
    "type": "library",
    "require": {
        "php": ">=8.3",
        "tiime/cross-industry-invoice": "^0.3.4",
        "tiime/universal-business-language": "^0.2.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tiime\\UniversalBusinessLanguageCrossIndustryInvoiceConversion\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tiime\\CrossIndustryInvoiceUniversalBusinessLanguageConversion\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "samueld-pdp",
            "email": "samuel.dechomets@tiimepdp.com"
        }
    ],
    "scripts": {
        "fix-cs": "vendor/bin/php-cs-fixer fix",
        "phpstan": "vendor/bin/phpstan analyse -l 9 src tests",
        "test": "vendor/bin/phpunit tests --display-warnings"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.64",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.4",
        "ext-dom": "*"
    }
